    "echarts-jsx": "^0.5.4",
    "file-type": "^20.4.1",
    "idb-keyval": "^6.2.1",
    "idea-react": "^2.0.0-rc.10",
    "koajax": "^3.1.2",
    "leaflet": "^1.9.4",
    "leaflet.chinatmsproviders": "^3.0.6",
    "less": "^4.3.0",
    "less-loader": "^12.2.0",
    "lodash": "^4.17.21",
    "marked": "^15.0.8",
    "mime": "^4.0.7",
    "mobx": "^6.13.7",
    "mobx-i18n": "^0.6.0",
    "mobx-lark": "^2.2.0",
    "mobx-react": "^9.2.0",
    "mobx-react-helper": "^0.3.4",
    "mobx-restful": "^2.1.0",
    "mobx-restful-table": "^2.1.1",
    "next": "^15.3.1",
    "next-pwa": "^5.6.0",
    "next-ssr-middleware": "^0.9.2",
