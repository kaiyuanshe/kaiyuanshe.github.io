  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@giscus/react": "^3.0.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.0.2",
    "@sentry/nextjs": "^8.36.0",
    "array-unique-proposal": "^0.3.4",
    "classnames": "^2.5.1",
    "copy-webpack-plugin": "^12.0.2",
    "echarts-jsx": "^0.5.4",
    "file-type": "^19.6.0",
    "idb-keyval": "^6.2.1",
    "idea-react": "^2.0.0-rc.8",
    "koajax": "^3.0.3",
    "leaflet": "^1.9.4",
    "leaflet.chinatmsproviders": "^3.0.6",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lodash": "^4.17.21",
    "marked": "^14.1.3",
    "mobx": "^6.13.5",
    "mobx-i18n": "^0.6.0",
    "mobx-lark": "^2.0.0-rc.3",
    "mobx-react": "^9.1.1",
    "mobx-react-helper": "^0.3.1",
    "mobx-restful": "^2.0.0",
    "mobx-restful-table": "^2.0.0",
    "next": "^15.0.2",
    "next-pwa": "^5.6.0",
    "next-ssr-middleware": "^0.8.8",
