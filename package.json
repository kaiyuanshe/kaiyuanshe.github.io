    "remark-gfm": "^4.0.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "web-utility": "^4.4.1",
    "webpack": "^5.96.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-typescript": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@kaiyuanshe/kys-service": "^1.0.0-rc.0",
    "@softonus/prettier-plugin-duplicate-remover": "^1.0.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/leaflet": "^1.9.14",
    "@types/lodash": "^4.17.13",
    "@types/mdx": "^2.0.13",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20.17.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.13.0",
    "eslint-config-next": "^15.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "get-git-folder": "^0.1.2",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.12.2"
  },
  "resolutions": {
    "mobx-restful": "$mobx-restful",
    "next": "$next"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "plugins": [
      "prettier-plugin-css-order",
      "@softonus/prettier-plugin-duplicate-remover"
    ]
  },
  "lint-staged": {
    "*.{html,md,less,json,yml,js,mjs,ts,tsx}": "prettier --write",
    "*.{js,mjs,ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "prepare": "husky",
    "install": "get-git-folder https://github.com/kaiyuanshe/service-configuration main kaiyuanshe.github.io  ||  true",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "lint-staged  &&  npm run lint  &&  tsc --noEmit",
    "pack-image": "docker build -t kys-ows:latest .",
    "container": "docker rm -f kys-ows  &&  docker run --name kys-ows -p 3000:3000 -d kys-ows:latest"
  }
}
